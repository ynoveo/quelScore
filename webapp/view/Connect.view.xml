<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="QuickStartApplication.controller.Connect">
	<App id="__app0">
		<pages>
			<Page title="Quel Score">
	            <content>
	                <sap.ui.layout:Grid xmlns:sap.ui.layout="sap.ui.layout" id="__grid0" class="sapUiSmallMarginTop">
	                    <sap.ui.layout:content xmlns:sap.ui.layout="sap.ui.layout">
	                        <Label text="Email" />
			                <Input
                				type="Email"
                				class="sapUiSmallMarginBottom"
                				placeholder="Saisissez votre email ..."
                			/>
				        </sap.ui.layout:content>
	                    <sap.ui.layout:content xmlns:sap.ui.layout="sap.ui.layout">
	                        <Label text="Password"/>
			                <Input type="Password" placeholder="Saisissez votre mot de passe ..."/>
				        </sap.ui.layout:content>
	                </sap.ui.layout:Grid>
	            </content>
	            <footer>
        			<Toolbar>
        				<ToolbarSpacer/>
        				<Button type="Emphasized" text="OK" press="onPress" />
        			</Toolbar>
        		</footer>
			</Page>
		</pages>
	</App>
</mvc:View>