{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "QuickStartApplication",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "Quelscore Coupe du monde 2018",
		"description": "Concours de pronostics pour la coupe du monde 2018",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "QuickStartApplication.view.Connect",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "QuickStartApplication.i18n.i18n"
				}
			},
			"global": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/Global.json"
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "QuickStartApplication.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "QuickStartApplication.view",
				"controlId": "__app0",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [{
				"pattern": "connect",
				"name": "connect",
				"target": "connect"
			}, {
				"pattern": "",
				"name": "appHome",
				"target": "home"
			}, {
				"pattern": "pronostics/{idUser}/pronostics/{oPseudo}",
				"name": "mesPronostics",
				"target": ["pronostics"]
			}, {
				"pattern": "compte",
				"name": "monCompte",
				"target": "compte"
			}, {
				"name": "mesGroupes",
				"pattern": "groupes",
				"greedy": false,
				"target": ["groupe"]
			}, {
				"name": "UserList",
				"pattern": "{groupId}",
				"greedy": false,
				"target": ["GroupUsers"]
			}],
			"targets": {
				"connect": {
					"viewName": "Connect",
					"viewLevel": 1
				},
				"home": {
					"viewPath": "QuickStartApplication.view",
					"viewName": "Home",
					"viewLevel": 2,
					"transition": "slide"
				},
				"pronostics": {
					"viewPath": "QuickStartApplication.view",
					"viewName": "Pronostics",
					"viewLevel": 2
				},
				"compte": {
					"viewPath": "QuickStartApplication.view",
					"viewName": "Compte",
					"viewLevel": 2
				},
				"groupe": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Groupe",
					"viewLevel": 2,
					"viewPath": "QuickStartApplication.view"
				},
				"GroupUsers": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "grouplist2",
					"viewLevel": 2
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}