                                <Button text="Deconnexion" width="200px" id="__button_disc" activeIcon="sap-icon://disconnected" type="Reject" icon="sap-icon://disconnected" iconFirst="true" iconDensityAware="true" press="onLogout" visible="false"/>
                            </contentLeft>
                    		<contentRight>
                            	<Button text="S'inscrire" width="100px" id="Bt_sub" type="Emphasized" press="onSubscribe"/>
                        	</contentRight>
                        

                        
                        </Bar>
                        <ObjectListItem counter="0" title="{/reponse/pseudo}" number="{/reponse/position}" numberUnit="{/reponse/nbpoints} pts" intro="Mon profil" icon="https://cdn4.iconfinder.com/data/icons/we-re-the-best/512/best-badge-cup-gold-medal-game-win-winner-gamification-first-award-acknowledge-acknowledgement-prize-victory-reward-conquest-premium-rank-ranking-gold-hero-star-quality-challenge-trophy-praise-victory-success-512.png" markFavorite="false" markFlagged="false" showMarkers="false" id="__item0" numberState="Success" busy="false" visible="false"/>
	                        <ObjectListItem ariaLabelledBy="__item4-label" type="Navigation" counter="0" intro="Groupes de joueurs" markFavorite="false" markFlagged="false" showMarkers="false" id="__item1" icon="sap-icon://group" press="onNavToGroupe" unread="false"/>
	                        <InputListItem type="Navigation" counter="0" label="Classement" id="__item4" press="onNavToCompte"/>
	                        <InputListItem type="Navigation" counter="0" label="Mes pronostics" id="__item5" detailPress="" press="onNavToPronostics"/>

